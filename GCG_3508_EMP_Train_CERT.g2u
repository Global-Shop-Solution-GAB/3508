Program.Sub.ButtonRouter.Start
Gui.F_Emp_Train..AlwaysOnTop(False)
F.Intrinsic.Control.Try
V.Local.sError.Declare

	V.Local.bBrowse.Declare(Boolean)
	V.Local.bSave.Declare(Boolean)
	V.Local.bDelete.Declare(Boolean)
	
	F.Intrinsic.String.IsInString(V.Args.ControlName, "brw", True, V.Local.bBrowse)
	F.Intrinsic.String.IsInString(V.Args.ControlName, "save", True, V.Local.bSave)
	F.Intrinsic.String.IsInString(V.Args.ControlName, "del", True, V.Local.bDelete)
	
	F.Intrinsic.Control.If(V.Local.bBrowse)
		F.Intrinsic.Control.CallSub("BrowseHandler", "ControlName", V.Args.ControlName)
		
	F.Intrinsic.Control.ElseIf(V.Local.bSave)
		F.Intrinsic.Control.CallSub("SaveHandler", "ControlName", V.Args.ControlName)
		
	F.Intrinsic.Control.ElseIf(V.Local.bDelete)
		F.Intrinsic.Control.CallSub("DeleteHandler", "ControlName", V.Args.ControlName)
		
	F.Intrinsic.Control.EndIf
	
F.Intrinsic.Control.Catch

    F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}", V.Ambient.ScriptPath, V.Ambient.Newline, V.Ambient.CurrentSubroutine, V.Ambient.ErrorNumber, V.Ambient.ErrorDescription, V.Ambient.GABVersion, V.Local.sError)
    F.Intrinsic.UI.MsgBox(V.Local.sError)
    F.Intrinsic.Control.End

F.Intrinsic.Control.EndTry

Program.Sub.ButtonRouter.End

Program.Sub.BrowseHandler.Start
F.Intrinsic.Control.Try
V.Local.sError.Declare

	V.Local.bTrainForm.Declare(Boolean)
	V.Local.bMaintForm.Declare(Boolean)
	V.Local.sFormName.Declare(String)
	V.Local.sTxtPrefix.Declare(String)
	
	V.Local.bJob.Declare(Boolean)
	V.Local.bCertProc.Declare(Boolean)
	V.Local.bCompany.Declare(Boolean)
	V.Local.bLevAccom.Declare(Boolean)
	V.Local.bEval.Declare(Boolean)
	V.Local.bEmpEQ.Declare(Boolean)
	V.Local.sType.Declare(String)
	V.Local.sBrowserTitle.Declare(String)
	V.Local.sColumnTitles.Declare(String)
	V.Local.sTxtSuffix.Declare(String)
	V.Local.iColumnWidths.Declare(Long)
	
	V.Local.sSQL.Declare(String)
	V.Local.sRet.Declare(String)
	
	V.Local.sTxtFull.Declare(String)
	
	F.Intrinsic.String.IsInString(V.Args.ControlName, "cmdT", True, V.Local.bTrainForm)
	F.Intrinsic.String.IsInString(V.Args.ControlName, "cmdM", True, V.Local.bMaintForm)
	
	F.Intrinsic.Control.If(V.Local.bMaintForm)
		V.Local.sFormName.Set("F_Maint")
		V.Local.sTxtPrefix.Set("txtM")
	F.Intrinsic.Control.ElseIf(V.Local.bTrainForm)
		V.Local.sFormName.Set("F_Emp_Train")
		V.Local.sTxtPrefix.Set("txtT")
	F.Intrinsic.Control.EndIf
	
	F.Intrinsic.String.IsInString(V.Args.ControlName, "Job", 		True, V.Local.bJob)
	F.Intrinsic.String.IsInString(V.Args.ControlName, "CertProc", 	True, V.Local.bCertProc)
	F.Intrinsic.String.IsInString(V.Args.ControlName, "Company", 	True, V.Local.bCompany)
	F.Intrinsic.String.IsInString(V.Args.ControlName, "LevAccom", 	True, V.Local.bLevAccom)
	F.Intrinsic.String.IsInString(V.Args.ControlName, "Eval", 		True, V.Local.bEval)
	F.Intrinsic.String.IsInString(V.Args.ControlName, "EmpEQ", 		True, V.Local.bEmpEQ)
	
	F.Intrinsic.Control.If(V.Local.bJob)
	
		V.Local.sType.Set("J")
		V.Local.sBrowserTitle.Set("Select a job.")
		V.Local.sColumnTitles.Set("Job")
		V.Local.sTxtSuffix.Set("Job")
	
	F.Intrinsic.Control.ElseIf(V.Local.bCertProc)
	
		V.Local.sType.Set("T")
		V.Local.sBrowserTitle.Set("Select a training certification / procedure.")
		V.Local.sColumnTitles.Set("Certification / Procedure")
		V.Local.sTxtSuffix.Set("CertProc")
	
	F.Intrinsic.Control.ElseIf(V.Local.bCompany)
	
		V.Local.sType.Set("C")
		V.Local.sBrowserTitle.Set("Select a company.")
