Program.Sub.cmdUpdPic_Click.Start
F.Intrinsic.Control.Try
V.Local.sError.Declare

	V.Local.sFile.Declare(String)
	V.Local.sEmpNum.Declare(String)
	V.Local.sSQL.Declare(string)
	V.Local.sRet.Declare(String)
	
	F.Intrinsic.UI.ShowOpenFileDialog("", "", "", V.Local.sFile)
	
	F.Intrinsic.Control.If(V.Local.sFile, <>, "***CANCEL***")
	
		V.Local.sFile.Set(V.Local.sFile.LCase)
		F.Intrinsic.Control.If(V.Local.sFile.right3, =, "png", "OR", V.Local.sFile.right3, =, "bmp", V.Local.sFile.right3, =, "jpg")
		
			V.Local.sEmpNum.Set(V.Screen.F_Emp_Train!txtEmpNum.Text)
			F.Intrinsic.String.Build("SELECT * FROM gcg_3508_emp_train WHERE type = 'P' AND employee = '{0}'", V.Local.sEmpNum, V.Local.sSQL)
			F.ODBC.Connection!Con.ExecuteAndReturn(V.Local.sSQL, V.Local.sRet)
			
			F.Intrinsic.Control.If(V.Local.sRet, <>, "")
				F.Intrinsic.String.Build("UPDATE gcg_3508_emp_train SET field_1 = '{0}' WHERE type = 'P' AND employee = '{1}'", V.Local.sFile, V.Local.sEmpNum, V.Local.sSQL)
			F.Intrinsic.Control.Else
				F.Intrinsic.String.Build("INSERT INTO gcg_3508_emp_train (employee, type, field_1) VALUES ('{0}', 'P', '{1}')", V.Local.sEmpNum, V.Local.sFile, V.Local.sSQL)
			F.Intrinsic.Control.EndIf
			
			F.ODBC.Connection!Con.Execute(V.Local.sSQL)
			
			Function.Intrinsic.Control.CallSub("SetPicture","sFQN",v.Local.sFile)
			
		F.Intrinsic.Control.Else
			F.Intrinsic.UI.Msgbox("Please select a valid file type (PNG, BMP, JPG).")
		F.Intrinsic.Control.EndIf
		
	F.Intrinsic.Control.EndIf
	
F.Intrinsic.Control.Catch

    F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}", V.Ambient.ScriptPath, V.Ambient.Newline, V.Ambient.CurrentSubroutine, V.Ambient.ErrorNumber, V.Ambient.ErrorDescription, V.Ambient.GABVersion, V.Local.sError)
