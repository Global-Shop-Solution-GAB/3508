Program.Sub.SetPicture.Start
	v.Static.dMaxWt.Declare(Float,333.0)
	v.Static.dMaxHt.Declare(Float,480.0)
	
	V.Local.sMetrics.Declare(String)
	v.Local.dAspect.Declare(Float)
	v.Local.iWt.Declare(Long)
	v.Local.iHt.Declare(Long)
	v.Local.iWtAdj.Declare(Long)
	v.Local.iHtAdj.Declare(Long)
	v.Local.iX.Declare(Long)
	v.Local.iY.Declare(Long)
						
	Function.Intrinsic.UI.GetImageMetrics(v.Args.sFQN,V.Local.sMetrics)
	Function.Intrinsic.String.Split(v.Local.sMetrics,"*!*",v.Local.sMetrics)
	v.Local.iWt.Set(v.Local.sMetrics(0))
	v.Local.iHt.Set(v.Local.sMetrics(1))
	
	' Get the height multiplier from the width vs max width
	Function.Intrinsic.Control.If(v.Local.iWt,<,v.Static.dMaxWt)
		v.Local.iWtAdj.Set(v.Local.iWt)
	Function.Intrinsic.Control.Else
		v.Local.iWtAdj.Set(v.Static.dMaxWt)
	Function.Intrinsic.Control.EndIf
				
	Function.Intrinsic.Math.Div(v.Local.iWtAdj,v.Local.iWt,v.Local.dAspect)
	Function.Intrinsic.Math.Mult(v.Local.iHt,v.Local.dAspect,v.Local.iHtAdj)
	Function.Intrinsic.Control.If(v.Local.iHtAdj,>,v.Static.dMaxHt)
		' Base on the height, not the width
		Function.Intrinsic.Control.If(v.Local.iHt,<,v.Static.dMaxHt)
			v.Local.iHtAdj.Set(v.Local.iHt)
		Function.Intrinsic.Control.Else
			v.Local.iHtAdj.Set(v.Static.dMaxHt)
		Function.Intrinsic.Control.EndIf
		
		Function.Intrinsic.Math.Div(v.Local.iHtAdj,v.Local.iHt,v.Local.dAspect)
		Function.Intrinsic.Math.Mult(v.Local.iWt,v.Local.dAspect,v.Local.iWtAdj)
		
	Function.Intrinsic.Control.Else
		' Use the width based values
		
	Function.Intrinsic.Control.EndIf
	
	Function.Intrinsic.Math.Mult(v.Local.iWtAdj,15,v.Local.iWtAdj)
	Function.Intrinsic.Math.Mult(v.Local.iHtAdj,15,v.Local.iHtAdj)
	
	gui.F_Emp_Train.picPhoto.Size(v.Local.iWtAdj,v.Local.iHtAdj)
	
	Function.Intrinsic.Math.Sub(v.Screen.F_Emp_Train.Width,v.Local.iWtAdj,375,v.Local.iX)
	gui.F_Emp_Train.lblPic.Position(v.Local.iX,210)
	Function.Intrinsic.Math.Add(v.Screen.F_Emp_Train!lblPic.Top,270,v.Local.iY)
	gui.F_Emp_Train.picPhoto.Position(v.Local.iX,v.Local.iY)
	
	Function.Intrinsic.Math.Add(v.Local.iHtAdj,v.Local.iY,150,v.Local.iY)
	gui.F_Emp_Train.cmdUpdPic.Position(v.Local.iX,v.Local.iY)
	
	Function.Intrinsic.Math.Sub(v.Local.iX,105,v.Local.iX)
	gui.F_Emp_Train.frm_left.Size(v.Local.iX,v.Screen.F_Emp_Train!frm_left.Height)
	
	GUI.F_Emp_Train.picPhoto.Picture(v.Args.sFQN)
Program.Sub.SetPicture.End

